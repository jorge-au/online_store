<!DOCTYPE html>
<html lang="es">
<head>
    <%- include('../partials/meta.ejs') %>
    <link rel="stylesheet" href="css/masProductos.css">
    <title>Celulares</title>
</head>
<body>
    <header>
        <%- include('../partials/nav.ejs') %>
    </header>
    <h1 style="text-align: center;">Celulares</h1>
    <main class="main">
        <% data.forEach((prod)=>{ %>
        <div class="masProductos__card">
            <a href=""><img src="<%= prod.imagen %>" alt="" class="masProductos__img"></a>
            <div class="body">
                <h5 class="body__titulo"><%= prod.nombre %></h5>
                <p class="body__descripcion"><%= prod.descripcion %></p>
                <p class="body__precio"><%= prod.precio %></p>
                <button class="btn__comprar">Comprar</button>
            </div>
        </div>
        <% }) %>
    </main>

    <!-- script de bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
</body>
</html>