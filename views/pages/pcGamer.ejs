<!DOCTYPE html>
<html lang="es">
  <head>
    <%- include('../partials/meta.ejs', {title: 'Pc Escritorio'}) %>
    <link rel="stylesheet" href="css/masProductos.css" />
  </head>
  <body>
    <header><%- include('../partials/nav.ejs') %></header>

    <h1 style="text-align: center">Gategoria Pc Gamers</h1>

    <main class="main">
      <% data.forEach((prod)=>{ %>
      <div class="masProductos__card">
        <a href=""
          ><img src="<%= prod.imagen %>" alt="" class="masProductos__img"
        /></a>
        <div class="body">
          <h5 class="body__titulo"><%= prod.nombre %></h5>
          <p class="body__descripcion"><%= prod.descripcion %></p>
          <p class="body__precio"><%= prod.precio %></p>
          <button class="btn__comprar">Comprar</button>
        </div>
      </div>
      <% }) %>
    </main>

    <!-- script de bootstrap -->
    <%- include('../partials/script_enlaces.ejs') %>
  </body>
</html>
